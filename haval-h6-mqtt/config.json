{
  "name": "GWM Brasil Haval H6 com MQTT",
  "version": "0.0.9",
  "slug": "hassio_haval_h6_to_mqtt",
  "description": "Integração do GWM Haval H6 com o Home Assistant usando MQTT. Esta integração não é aplicável aos modelos Ora.",
  "init": false,
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/havaleiros/hassio-haval-h6-to-mqtt",
  "map": ["config:rw"],
  "options": {
    "data_path": "/config/haval_h6_mqtt",
    "refresh_minutes": 5,
    "mqtt_server": "mqtt://homeassistant.local:1883",
    "pressure_unit": "psi"
  },
  "schema": {
    "data_path": "str",
    "haval_username": "email",
    "haval_password": "password",
    "haval_vin": "str",
    "mqtt_server": "str?",
    "mqtt_user": "str",
    "mqtt_pass": "password?",
    "refresh_minutes": "int",
    "pressure_unit": "list(kPa|psi)"
  }
}
